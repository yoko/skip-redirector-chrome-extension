{
	"manifest_version": 2,
	"name": "Skip Redirector",
	"version": "1.1.2",
	"description": "Skip some URL redirector and age check.",
	"background": {
		"scripts": [ "background.js" ]
	},
	"options_page": "options.html",
	"permissions": [
		"http://wedata.net/"
	],
	"content_scripts": [
		{
			"js"     : ["skip_redirector.js"],
			"matches": ["http://*/*", "https://*/*"]
		}
	]
}
